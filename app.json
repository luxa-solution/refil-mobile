{
  "expo": {
    "name": "refil-mobile",
    "slug": "refil-mobile",
    "version": "1.0.0",
    
        "newArchEnabled": true,
    
    
      "scheme": "refil-mobile",
      "platforms": ["ios", "android"],
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
      "expo-router"
      
        ,"expo-localization"
      
    ],
      "experiments": {
        "typedRoutes": true
        
          ,"tsconfigPaths": true
        
      },
    
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    
    "userInterfaceStyle": "automatic",
     
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
