    "@tanstack/react-query": "^5.90.7",
    "axios": "^1.13.2",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.13",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
